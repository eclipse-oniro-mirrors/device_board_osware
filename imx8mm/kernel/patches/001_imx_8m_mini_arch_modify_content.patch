-void spectre_bhb_enable_mitigation(const struct arm64_cpu_capabilities *entry)
-{
-	enum mitigation_state fw_state, state = SPECTRE_VULNERABLE;
-
-	if (!is_spectre_bhb_affected(entry, SCOPE_LOCAL_CPU))
